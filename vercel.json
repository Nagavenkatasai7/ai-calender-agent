{
  "version": 2,
  "builds": [
    {
      "src": "dist/serverlessApp.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "10mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "/dist/serverlessApp.js"
    },
    {
      "src": "/api/voice/(.*)",
      "dest": "/dist/serverlessApp.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/serverlessApp.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 